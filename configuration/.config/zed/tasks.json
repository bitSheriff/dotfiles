[
    {
        "label": "start lazygit",
        "command": "cd '$ZED_WORKTREE_ROOT' && lazygit",
        "tags": ["devops"]
    },

    {
        "label": "Rust build",
        "command": "cd '$ZED_CUSTOM_RUST_PACKAGE' && cargo build",
        "use_new_terminal": false,
        "tags": ["rust"]
    },
    {
        "label": "television file finder",
        "command": "zed \"$(tv files)\"",
        "hide": "always",
        "allow_concurrent_runs": true,
        "use_new_terminal": true
    }
]
