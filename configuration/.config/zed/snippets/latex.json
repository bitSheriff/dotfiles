{
    "Bold": {
        "prefix": "bf",
        "body": "\\textbf{$1}",
        "description": "Bold"
    },

    "Italic": {
        "prefix": "it",
        "body": "\\textit{$1}",
        "description": "Italic"
    },

    "Underline": {
        "prefix": "ul",
        "body": "\\underline{$1}",
        "description": "Underline"
    },

    "Monospaced": {
        "prefix": "tt",
        "body": "\\texttt{$1}",
        "description": "Monospaced"
    },

    "Code Listing": {
        "prefix": "code listing",
        "body": [
            "\\begin{lstlisting}[language=LANG, label=lst:LABEL, caption={CAPTION}]",
            "$1",
            "\\end{lstlisting}"
        ],
        "description": "Code Listing"
    },

    "Code Inline": {
        "prefix": "code inline",
        "body": "\\lstinline{$1}",
        "description": "Code Inline"
    },

    "Todo Inline": {
        "prefix": "todo",
        "body": "\\todo[inline]{$1}",
        "description": "Todo Inline"
    },

    "Todo": {
        "prefix": "todo",
        "body": "\\todo{$1}",
        "description": "Todo"
    },

    "Equation": {
        "prefix": "eq",
        "body": [
            "\\begin{equation}",
            "    $1",
            "\\end{equation}"
        ],
        "description": "Simeple Equation"
    },

    "Itemize": {
        "prefix": "itemi",
        "body": [
            "\\begin{itemize}",
            "    \\item $1",
            "\\end{itemize}"
        ],
        "description": "Itemize"
    },

    "Beamer: Simple Frame": {
        "prefix": "frame",
        "body": [
            "\\begin{frame}{}",
            "    $1",
            "\\end{frame}"
        ],
        "description": "Simeple Frame"
    },

    "Beamer: Frame w break": {
    "prefix": "frame",
    "body": [
        "\\begin{frame}[allowframebreaks]{}",
        "    $1",
        "\\framebreak",
        "\\end{frame}"
    ],
    "description": "Frame with Framebreak"
    },

    "Beamer: Frame w Column": {
    "prefix": "frame col 2",
    "body": [
        "\\begin{frame}{}",
        "\\begin{columns}",
        "\\begin{column}{0.5\\textwidth}",
        "    ",
        "\\end{column}",
        "\\begin{column}{0.5\\textwidth}",
        "    ",
        "\\end{column}",
        "\\end{columns}",
        "\\end{frame}"
    ],
    "description": "Frame Two columns"
    }

}
