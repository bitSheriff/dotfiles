{
    "Bold": {
        "prefix": "bf",
        "body": "\\textbf{$1}",
        "description": "Bold"
    },

    "Italic": {
        "prefix": "it",
        "body": "\\textit{$1}",
        "description": "Italic"
    },

    "Underline": {
        "prefix": "ul",
        "body": "\\underline{$1}",
        "description": "Underline"
    },

    "Monospaced": {
        "prefix": "tt",
        "body": "\\texttt{$1}",
        "description": "Monospaced"
    },

    "Code Listing": {
        "prefix": "code listing",
        "body": [
            "\\begin{lstlisting}[language=LANG, label=lst:LABEL, caption={CAPTION}]",
            "$1",
            "\\end{lstlisting}"
        ],
        "description": "Code Listing"
    },

    "Code Inline": {
        "prefix": "code inline",
        "body": "\\lstinline{$1}",
        "description": "Code Inline"
    }
}
