{
    "Underline": {
        "prefix": "ul",
        "body": "<u>$1</u>",
        "description": "Underline"
    },

    "Todo": {
        "prefix": "todo",
        "body": "- [ ] $1"
    },

    "Link": {
        "prefix": "link",
        "body": "[$1]($2)",
        "description": "URL Link"
    },

    "h1": {
        "prefix": "h1",
        "body": "# $1",
        "description": "Header 1"
    },

    "h2": {
        "prefix": "h2",
        "body": "## $1",
        "description": "Header 2"
    },

    "h3": {
        "prefix": "h3",
        "body": "### $1",
        "description": "Header 3"
    },

    "h4": {
        "prefix": "h4",
        "body": "#### $1",
        "description": "Header 4"
    },

    "h5": {
        "prefix": "h5",
        "body": "##### $1",
        "description": "Header 5"
    },

    "h6": {
        "prefix": "h6",
        "body": "###### $1",
        "description": "Header 6"
    },

    "Code": {
        "prefix": "code",
        "body": "`$1`",
        "description": "Inline Code"
    },

    "Code Block": {
        "prefix": "code block",
        "body": ["```${1:Language}", "$2", "```"],
        "description": "Code Block"
    },

    "Hugo Comment": {
        "prefix": "hugo comment",
        "body": ["<!--", "    ${1:Comment}", "-->"],
        "description": "Hugo Comment"
    },

    "Hugo Github Repo": {
        "prefix": "hugo github repo",
        "body": ["{{< github repo=\"NAME/REPO\" >}}"]
    },

    "Hugo Alert": {
        "prefix": "hugo alert",
        "body": [
            "{{< alert icon=\"fire\" cardColor=\"#e63946\" iconColor=\"#1d3557\" textColor=\"#f1faee\" >}}",
            "    ${1:Alert}",
            "{{< /alert >}}"
        ]
    },

    "Hugo Article": {
        "prefix": "hugo article",
        "body": ["{{< article link=\"/docs/welcome/\" >}}"]
    },

    "Hugo Codeimporter": {
        "prefix": "hugo codeimporter",
        "body": [
            "{{< codeimporter url=\"https://raw.githubusercontent.com/\" type=\"shell\" >}}"
        ]
    }
}
